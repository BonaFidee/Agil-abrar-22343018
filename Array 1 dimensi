#include <iostream>

using namespace std;

int main (){
    int data[5];
    cout << "Nama : Agil abrar"<< endl;
    cout << "Nim : 22343018" << endl;
    cout<< "----------------------------------------------------------------------------------"<<endl;

    for (int i=0; i<5; i++){
        cout<<"Masukkan Angka baris ke "<<i<<" = ";
        cin>>data[i];
    }
    cout<<endl;

    for (int i=0; i<5; i++){
        cout<<"Angka  baris ke-"<<i<<" = "<<data[i];
        cout<<endl;
    }
    return 0;
}
